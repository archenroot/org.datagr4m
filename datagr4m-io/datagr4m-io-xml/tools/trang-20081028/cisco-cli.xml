<cli name="cisco">
	<login>
		<user string="Username: "/>
		<password string="Password: "/>
		<enable string="Password:"/>
	</login>
	<prompt>
		<default string=">"/>
		<root string="#"/>
	</prompt>
	<commands>
		<command type="conf" line="show run"/>
		<command type="mat" line="show mac address table"/>
		<command type="ping" line="ping ${ip}" />
	</commands>
	<pager>
		<next string=" --More-- "/>
	</pager>
	<cleaner>
		<replace from="" to=""/>
		<replace from="" to=""/>
		<replace from="" to=""/>
	</cleaner>
</cli>